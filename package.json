{
  "name": "template-node-cli",
  "version": "0.0.1",
  "description": "template-node-cli",
  "scripts": {
    "build": "gulp --require sucrase/register/ts --gulpfile build/gulpfile.ts",
    "clear": "rimraf dist",
    "link": "npm link",
    "push": "git push gitee master && git push githup master",
    "update:version": "sucrase-node build/utils/version.ts",
    "log": "conventional-changelog -p custom-config  -i CHANGELOG.md -s -n ./changelog-option.js"
  },
  "bin": {
    "template-cli": "./dist/main.js"
  },
  "author": "Yonghu",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/gulp": "^4.0.9",
    "@types/inquirer": "^8.2.0",
    "@types/inquirer-autocomplete-prompt": "^1.3.3",
    "@types/node": "^17.0.21",
    "@types/shelljs": "^0.8.11",
    "commitizen": "^4.1.2",
    "compare-func": "^2.0.0",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.2.0",
    "gulp": "^4.0.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "sucrase": "^3.20.3",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "^9.1.0",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.1",
    "ora": "^5.1.0",
    "shelljs": "^0.8.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}